    "@types/nodemailer": "^7.0.2",
    "@types/ws": "^8.18.1",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "connect-flash": "^0.1.1",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-ejs-layouts": "^2.5.1",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.2",
