{
  "name": "billing-app",
  "version": "2.4.16",
  "description": "Billing System Management",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "npx tsc",
    "start": "node dist/server.js",
    "css:build": "tailwindcss -i ./src/styles/tailwind.css -o ./public/assets/styles.css --minify",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:start:cmd": "start-pm2.bat",
    "pm2:start:bypass": "start-pm2-cmd.bat",
    "pm2:restart": "node src/scripts/pm2-manager.js",
    "pm2:restart:auto": "restart-pm2-auto.bat",
    "pm2:reload": "pm2 reload billing-app",
    "pm2:stop": "pm2 stop billing-app",
    "pm2:delete": "pm2 delete billing-app",
    "pm2:logs": "pm2 logs billing-app",
    "pm2:status": "pm2 list",
    "pm2:monit": "pm2 monit",
    "deploy": "npm run build && pm2 reload billing-app",
    "deploy:ubuntu": "npm run build && pm2 delete billing-app || true && pm2 start ecosystem.config.js --env production",
    "test:bot": "node test-bot-response.js",
    "rebuild": "npm run build",
    "rebuild:restart": "npm run build && npm run pm2:restart",
    "monitoring:init": "ts-node src/scripts/initNetworkMonitoring.ts",
    "monitoring:daemon": "ts-node src/scripts/monitoringDaemon.ts",
    "release": "ts-node src/scripts/release.ts",
    "setup:db": "ts-node src/scripts/setup-db.ts",
    "cleanup": "ts-node src/scripts/smart_cleanup.ts",
    "backup:db": "ts-node src/scripts/run-backup.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
