    "midtrans-client": "^1.4.3",
    "mikronode": "^2.3.11",
    "mime-types": "^3.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.15.2",
    "node-cron": "^3.0.3",
    "node-routeros": "^1.6.8",
    "node-ssh": "^12.0.5",
    "node-telegram-bot-api": "^0.66.0",
    "node-thermal-printer": "^4.4.4",
